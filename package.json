{
  "name": "breeze-next",
  "version": "1.0.0",
  "description": "Next version of Breeze, built using TypeScript 2.",
  "main": "app.js",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "cross-spawn": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-task-listing": "^1.0.1",
    "gutil": "^1.6.4",
    "jasmine": "^2.5.2",
    "jasmine-node": "^1.14.5",
    "lodash": "^4.16.4",
    "q": "^1.4.1",
    "ts-loader": "^0.9.5",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "uglifyjs": "^2.4.10",
    "rollup": "^0.36.3",
    "rollup-plugin-multi-entry": "^2.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-breeze": "webpack --progress --profile --colors --display-error-details --display-cached",
    "webpack-breeze-adapters": "webpack --config webpack.adapters.config.js --progress --profile --colors --display-error-details --display-cached",
    "webpack-watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "rollup-only": "node_modules/.bin/rollup.cmd -c rollup.js",
    "tsc-es5-only": "tsc --target es5 --allowJs build/breeze.es2015.js --out build/breeze.debug.js",
    "tsc-2015": "tsc -p tsconfig.es2015.json",
    "minify": "uglifyjs build/breeze.debug.js --screw-ie8 --compress --mangle --output build/breeze.min.js",
    "rollup": "npm run tsc-2015 && npm run rollup-only && npm run tsc-es5-only && npm run minify",
    "tsdoc": "typedoc --out ./docs/  --excludePrivate --mode file --readme ./readme.doc.md ./src/breeze.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/breeze-sandbox/breeze-next.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/breeze-sandbox/breeze-next/issues"
  },
  "homepage": "https://github.com/breeze-sandbox/breeze-next#readme"
}
